<?xml version="1.0" encoding="utf-8"?>
     <configuration>
     <system.webServer>
       <handlers>            
        <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" 
          scriptProcessor="c:\python\python.exe|c:\python\lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
       </handlers>
     </system.webServer>
     <appSettings>       
       <add key="WSGI_HANDLER" value="main.app" />
       <!-- <add key="PYTHONPATH" value="C:\DEV\botxx" /> -->
     </appSettings>
    <system.webServer>
        <directoryBrowse enabled="false" />
    </system.webServer>
     </configuration>
